{
  "name": "gg-ssh-client",
  "version": "1.0.0",
  "description": "A lightweight, cross-platform SSH client",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "set NODE_ENV=development && electron .",
    "build": "electron-builder -c electron-builder.json",
    "build:win": "electron-builder -c electron-builder.json --win",
    "build:mac": "electron-builder -c electron-builder.json --mac",
    "build:linux": "electron-builder -c electron-builder.json --linux"
  },
  "dependencies": {
    "@xterm/xterm": "5.5.0",
    "@xterm/addon-fit": "0.10.0",
    "@xterm/addon-web-links": "0.10.0",
    "electron-store": "^8.1.0",
    "node-ssh": "^13.0.1",
    "ssh2": "^1.11.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "electron": "^27.0.0",
    "electron-builder": "^24.6.3"
  }
}